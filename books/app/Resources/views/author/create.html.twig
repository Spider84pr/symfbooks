{# app/Resources/views/product/create.html.twig #}

{% block flash_messages %}
    {% for type, messages in app.session.flashbag.all() %}
        {% for message in messages %}
            <div class="alert alert-{{ type }} alert-dismissible" role="alert">
                {{ message | raw }}
            </div>
        {% endfor %}
    {% endfor %}
{% endblock %}

{{ form_start(form) }}
{{ form_widget(form) }}
{{ form_end(form) }}